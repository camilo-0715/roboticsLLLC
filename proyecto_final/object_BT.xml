 <root main_tree_to_execute = "MainTree" >
     <BehaviorTree ID="MainTree">
        <Sequence name="root_sequence">
            <ReactiveSequence name="object_sequence"> 
              <Fallback name="check_object_position">
                <InFront        name="in_front"        object="{object}"/>
                <Look4object         name="Look4object"          object="{object}"/>
              </Fallback>
              <ApproachObject name="approach_object" object="{object}"/> 
            </ReactiveSequence>
        </Sequence>
     </BehaviorTree>
 </root>